; Flame

[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur
AddReg    = Scheme.Reg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"
Scheme.Txt = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%cross%,%10%\%CUR_DIR%\%text%,%10%\%CUR_DIR%\%hand%,%10%\%CUR_DIR%\%unavailable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%"

; Installed files

[Scheme.Cur]
pntr.cur
hlp.cur
wrk.cur
bsy.cur
txt.cur
unvl.cur
vrt.cur
hrz.cur
dgn1.cur
dgn2.cur
mv.cur
lnk.cur
crs.cur
hnd.cur
alt.cur


[Strings]
CUR_DIR = "Cursors\Flame"
SCHEME_NAME = "Flame"
pointer = "pntr.cur"
help = "hlp.cur"
work = "wrk.cur"
busy = "bsy.cur"
text = "txt.cur"
unavailable = "unvl.cur"
vert = "vrt.cur"
horz = "hrz.cur"
dgn1 = "dgn1.cur"
dgn2 = "dgn2.cur
move = "mv.cur"
link = "lnk.cur"
cross = "crs.cur"
